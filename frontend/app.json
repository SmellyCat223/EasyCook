{
  "expo": {
    "name": "EasyCook",
    "slug": "EasyCook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp-scheme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo2.png",
      "resizeMode": "cover",
      "backgroundColor": "black"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smellycat.EasyCook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smellycat.EasyCook"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f724d1d-70b2-4dfd-a179-dfb39806d16d"
      }
    }
  }
}
