{
  "expo": {
    "name": "EasyCook",
    "slug": "easycook-orbital",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp-scheme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo2.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smellycat.EasyCook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smellycat.EasyCook"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "25e24c8e-9e9f-402f-8c5c-3d3961f8b5cd"
      }
    },
    "owner": "easycook_dev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/25e24c8e-9e9f-402f-8c5c-3d3961f8b5cd"
    }
  }
}